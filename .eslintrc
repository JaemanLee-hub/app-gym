{
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6, //Js 버젼
      "sourceType": "module"
    },
    "plugins": ["import", "node", "promise", "standard","jest"],
    "env": { // 코드환경
      "browser": true,
      "node": true,
      "jest/globals": true
    },
    "extends": [ // 디폴트 룰
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "rules": { // 실질적인 커스텀룰
      "no-console": "warn"
    }
}